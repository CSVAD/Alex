//unhappy Blockhead from Gumby

size(800,800);
background(255,255,255);
rectMode(CENTER);
fill(255,0,0);
rect(width/2,height/2,600,600);
ellipseMode(CENTER);
fill(255,255,255);
circle(width/2-120,height/2-120, 100);
circle(width/2+120,height/2-120, 100);
fill(0,0,0);
circle(width/2-120,height/2-120,10);
circle(width/2+120,height/2-120,10);
strokeWeight(10);
line(width/2-135,height/2-185,width/2-55,height/2-135);
line(width/2+55,height/2-135,width/2+135,height/2-185);
fill(255, 255, 0);
noStroke();
circle(width/2,height/2,50);
ellipse(width/2,height/2 + 195, 500, 200);
fill(255, 0, 0);
ellipse(width/2,height/2 + 210, 500, 170);
